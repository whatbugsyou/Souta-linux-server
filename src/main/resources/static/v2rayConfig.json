{
  "log": {
    "access": "",
    "error": "",
    "loglevel": "none"
  },
  "api": {
    "services": [
      "HandlerService"
    ],
    "tag": "api"
  },
  "inboundDetour": [
    {
      "listen": "127.0.0.1",
      "port": 8080,
      "protocol": "dokodemo-door",
      "settings": {
        "address": "127.0.0.1"
      },
      "tag": "api"
    }
  ],
  "routing": {
    "rules": [
      {
        "inboundTag": [
          "api"
        ],
        "outboundTag": "api",
        "type": "field"
      },
      {
        "inboundTag": [
          "dns_inbound"
        ],
        "balancerTag": "dnsBalancer",
        "type": "field"
      }
    ],
    "balancers": [
      {
        "tag": "dnsBalancer",
        "selector": ["out"]
      }
    ]
  },
  "dns": {
    "servers": [
      "114.114.114.114",
      "8.8.8.8"
    ],
    "clientIp": "1.2.3.4",
    "tag": "dns_inbound"
  }
}